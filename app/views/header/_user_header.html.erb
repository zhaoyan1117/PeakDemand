<% if user_signed_in? %>
	Hello <b><%= @user.name %></b>, your identity: <b><%= @user.identity %></b>
	<%= link_to 'Log out', destroy_user_session_path, :method => :delete %>
	<%= link_to 'Edit my account', edit_user_registration_path(@user) %>
<% else %>
	<%= link_to 'Sign in', new_user_session_path %>
<% end %>
