<%= form_for :resource, :method => method, :url => url do |f|  %>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br>

	<%= f.label :description %><br>
	<%= f.text_area :description %><br>

	<%= f.label :available_on %><br>
	<%= f.date_select :start_at %><br>
	
	<%= f.label :end_at %><br>
	<%= f.date_select :end_at %><br>

	<%= submit_tag("Submit") %>
<% end %>