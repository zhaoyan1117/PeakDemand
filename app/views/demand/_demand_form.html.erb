<%= form_for :demand, :method => method, :url => url do |f|  %>

	<%= f.label :start_at %><br>
	<%= f.date_select :start_at %><br>
	<br>

	<%= f.label :short_description %><br>
	<%= f.text_field :short_description %><br>
	<br>
	
	<%= f.label :end_at %><br>
	<%= f.date_select :end_at %><br>
	<br>
	
	<%= f.label :description %><br>
	<%= f.text_area :description %><br>
	<br>
	
	<%= f.label :intensity %><br>
	<% intensities.each do |i| %>
		<%= f.label i %>
		<%= f.radio_button :intensity, i %><br>
	<% end %>
	<br>
	
	<%= submit_tag("Submit") %>
<% end %>